<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration 
DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pack.Country" table="country">
		<id name="id" type="int" column="Countryid">
			<generator class="increment"></generator>
		</id>
	<property name="name" type="string" column="name"></property>
	<property name="population" type="int" column="population"></property>	
	
	</class>




</hibernate-mapping>
</hibernate-configuration>